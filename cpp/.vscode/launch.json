    "version": "0.2.0",
    "configurations": [
        {
            "name": "C/C++: cl.exe luncher active file",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${fileDirname}\\build\\${fileBasenameNoExtension}.exe",
            "args": ["list"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "externalTerminal",
            "preLaunchTask": "C/C++: cl.exe build active file"
        },
        {
            "name": "C/C++: cmake debug luncher nvme.exe",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${fileDirname}\\build\\nvme.exe",
            "args": ["-b", "3", "id-ctrl"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "externalTerminal",
            "preLaunchTask": "CMake: build"
        },
    ],
}